<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&sensor=false"></script>
<script>
  function initialize() {
    $('#map-canvas').attr("style", "width: 940px !important; height: 300px; position: relative; background-color: rgb(229, 227, 223); overflow: hidden;");
    var myLatlng = new google.maps.LatLng(22.636599,75.815493);
    var mapOptions = {
      zoom: 10,
      center: myLatlng,
      mapTypeId: google.maps.MapTypeId.ROADMAP
    }
    var map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);

    var marker = new google.maps.Marker({
        position: myLatlng,
        map: map,
        title: 'Rau, Indore, India'
    });
  }

  google.maps.event.addDomListener(window, 'load', initialize);
</script>

